		<dependency>
			<groupId>com.techsure</groupId>
			<artifactId>neatlogic-inspect-base</artifactId>
		</dependency>
		<dependency>
			<groupId>com.techsure</groupId>
			<artifactId>neatlogic-autoexec-base</artifactId>
